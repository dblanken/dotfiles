[user]
  name = David Blankenship
  email = dblanken@iu.edu
[hub]
  host = github.iu.edu
[core]
  autocrlf = input
  excludefiles = /Users/dblanken/.gitignore
[push]
  default = upstream
[merge]
  ff = only
[fetch]
  prune = true
[commit]
  template = ~/.gitmessage
[alias]
  b = branch
  ca = commit -v -a
  cb = checkout -b
  co = checkout
  unstage = reset
